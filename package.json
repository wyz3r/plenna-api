{
  "name": "plenna",
  "version": "1.0.0",
  "description": "is a api for the code challenge in plena ",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "test": "jest",
    "format": "prettier --write .",
    "init:collections": "ts-node src/scripts/init-collections.ts",
    "seed:basic": "ts-node src/scripts/seed-basic.ts",
    "seed:massive": "ts-node src/scripts/seed-massive.ts",
    "reset:db": "ts-node src/scripts/reset-db.ts",
    "test:coverage": "jest --coverage",
    "stress:users": "node src/scripts/stress-user.js",
    "stress:get": "node src/scripts/stress-test.js"
  },
  "author": "Leonel Islas Gonzalez",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongoose": "^9.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "autocannon": "^8.0.0",
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
